<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>ros_3d_attractor</name>
  <version>0.0.1</version>
  <description>A ROS2 package that implements a node that generates force commands suitable for attracting a robotic effector to a point, line, or plane embedded in a 3D space</description>
  <maintainer email="nml@whit.contact">a.whit</maintainer>
  <license>Mozilla Public License 2.0</license>

  <test_depend>ament_copyright</test_depend>
  <test_depend>ament_flake8</test_depend>
  <test_depend>ament_pep257</test_depend>
  <test_depend>python3-pytest</test_depend>

  <export>
    <build_type>ament_python</build_type>
  </export>
</package>
